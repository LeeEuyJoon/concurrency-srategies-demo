          "expr": "histogram_quantile(0.90, sum(rate(concurrency_retry_distribution_seconds_bucket{strategy!=\"UNKNOWN\",ep!=\"UNKNOWN\"}[1m])) by (le, strategy, ep))",
          "legendFormat": "{{strategy}} · {{ep}}"
        }
      ],
      "fieldConfig": { "defaults": { "unit": "short", "decimals": 0 }, "overrides": [] }
    },
    {
