{
  "uid": "concurrency-lock-wait",
  "title": "Concurrency • Lock Wait & Retry",
  "timezone": "browser",
  "schemaVersion": 38,
  "version": 1,
  "refresh": "5s",
  "panels": [
    {
      "type": "timeseries",
      "title": "Average lock wait (by strategy, ep)",
      "gridPos": { "x": 0, "y": 0, "w": 12, "h": 8 },
      "targets": [
        {
          "refId": "A",
          "datasource": { "type": "prometheus", "uid": "prometheus" },
          "expr": "(sum by (strategy, ep) (rate(concurrency_lock_wait_seconds_sum{strategy!=\"UNKNOWN\",ep!=\"UNKNOWN\"}[1m]))) / clamp_min(sum by (strategy, ep) (rate(concurrency_lock_wait_seconds_count{strategy!=\"UNKNOWN\",ep!=\"UNKNOWN\"}[1m])), 1)",
          "legendFormat": "{{strategy}} · {{ep}}"
        }
      ],
      "fieldConfig": { "defaults": { "unit": "s" }, "overrides": [] }
    },
    {
      "type": "timeseries",
      "title": "Average retry count (by strategy, ep)",
      "gridPos": { "x": 12, "y": 0, "w": 12, "h": 8 },
      "targets": [
        {
          "refId": "A",
          "datasource": { "type": "prometheus", "uid": "prometheus" },
          "expr": "rate(concurrency_retry_total{strategy!=\"UNKNOWN\",ep!=\"UNKNOWN\"}[1m]) / clamp_min(rate(concurrency_lock_wait_seconds_count{strategy!=\"UNKNOWN\",ep!=\"UNKNOWN\"}[1m]), 1)",
          "legendFormat": "{{strategy}} · {{ep}}"
        }
      ],
      "fieldConfig": { "defaults": { "unit": "short", "decimals": 1 }, "overrides": [] }
    },
    {
      "type": "timeseries",
      "title": "p90 lock wait (by strategy, ep)",
      "gridPos": { "x": 0, "y": 8, "w": 12, "h": 8 },
      "targets": [
        {
          "refId": "A",
          "datasource": { "type": "prometheus", "uid": "prometheus" },
          "expr": "histogram_quantile(0.90, sum(rate(concurrency_lock_wait_seconds_bucket{strategy!=\"UNKNOWN\",ep!=\"UNKNOWN\"}[1m])) by (le, strategy, ep))",
          "legendFormat": "{{strategy}} · {{ep}}"
        }
      ],
      "fieldConfig": { "defaults": { "unit": "s" }, "overrides": [] }
    },
    {
      "type": "timeseries",
      "title": "p90 retry count (by strategy, ep)",
      "gridPos": { "x": 12, "y": 8, "w": 12, "h": 8 },
      "targets": [
        {
          "refId": "A",
